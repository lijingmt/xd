#include <globals.h>
#include <gamelib/include/gamelib.h>
inherit WAP_BAOSHI;
void create(){
	name=object_name(this_object());
	name_cn="【冰】猫眼石";
	unit="颗";
	picture="bingmaoyanshi";
	desc="增加毒素法术伤害15点\n";
	set_color("blue");
	set_item_canEquip(1);
	set_item_canDrop(1);
	set_item_canGet(1);
	set_item_canTrade(1);
	set_item_canSend(1);
	set_item_canStorage(1);
	set_value(50);
	set_item_canLevel(50);
	set_du_mofa_attack_add(15);
	set_item_kind("pusu");
}
