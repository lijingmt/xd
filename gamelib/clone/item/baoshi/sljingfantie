#include <globals.h>
#include <gamelib/include/gamelib.h>
inherit WAP_BAOSHI;
void create(){
	name=object_name(this_object());
	name_cn="闪亮晶钒铁";
	unit="颗";
	picture="sljingfantie";
	desc="增加幸运200点\n";
	set_color("red");
	set_item_canEquip(1);
	set_item_canDrop(1);
	set_item_canGet(1);
	set_item_canTrade(1);
	set_item_canSend(1);
	set_item_canStorage(1);
	set_value(150);
	set_item_canLevel(60);
	set_lunck_add(200);
	set_item_kind("shanliang");
}
