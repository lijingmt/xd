#include <globals.h>
#include <gamelib/include/gamelib.h>
inherit WAP_BAOSHI;
void create(){
	name=object_name(this_object());
	name_cn="闪亮晶坚石";
	unit="颗";
	picture="sljingjianshi";
	desc="增加毒素伤害25点\n";
	set_color("red");
	set_item_canEquip(1);
	set_item_canDrop(1);
	set_item_canGet(1);
	set_item_canTrade(1);
	set_item_canSend(1);
	set_item_canStorage(1);
	set_value(150);
	set_item_canLevel(60);
	set_attack_dusu_add(25);
	set_item_kind("shanliang");
}
