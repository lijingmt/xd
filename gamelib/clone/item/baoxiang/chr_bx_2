#include <globals.h>
#include <gamelib/include/gamelib.h>
inherit WAP_BAOXIANG;
void create(){
	name=object_name(this_object());
	name_cn="二级圣诞宝箱";
	unit="个";
	set_item_canDrop(1);
	set_item_canGet(1);
	set_item_canTrade(1);
	set_item_canSend(1);
	set_item_canStorage(1);
	set_item_level(2);
	desc="看起来非常爽的样子\n";
}
