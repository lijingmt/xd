#include <globals.h>
#include <gamelib/include/gamelib.h>
inherit WAP_HONGBAO;
void create(){
	name=object_name(this_object());
	name_cn="新年红包";
	unit="个";
	set_item_canDrop(1);
	set_item_canGet(1);
	set_item_canTrade(0);
	set_item_canSend(0);
	set_item_canStorage(1);
	set_item_level(10);
	desc="新年快乐！\n";
}
