#include <globals.h>
#include <gamelib/include/gamelib.h>
inherit WAP_BOOK;
void create(){
	name=object_name(this_object());
	name_cn="【妖】鬼踪一级";
	unit="本";
	picture=name;
	desc="鬼魅的步伐，可以增加使用者的闪避率\n";
	set_item_canDrop(1);
	set_item_canGet(1);
	set_item_canTrade(1);
	set_item_canSend(1);
	set_item_canStorage(1);
	value=50;
	skill_bname="guizong";
	level_limit=9;
	profe_read_limit="影鬼";
	beidong_level=1;
	need_yushi=5;
	need_money=50;
}
int read(){
	int result=::beidong_read();
	if(read_flag==0){
		remove();
	}
	return result;
}
