#include <globals.h>
#include <gamelib/include/gamelib.h>
inherit WAP_BOOK;
void create(){
	name=object_name(this_object());
	name_cn="【诅】击颅11级";
	unit="本";
	picture=name;
	desc="攻击敌人的后脑，导致对方的智力降低\n";
	set_item_canDrop(1);
	set_item_canGet(1);
	set_item_canTrade(1);
	set_item_canSend(1);
	set_item_canStorage(1);
	value=50;
	skill_bname="jilu";
	level_limit=59;
	skill_level = 11; 
	profe_read_limit="影鬼";
}
int read(){
	int result=::read();
	if(read_flag==0){
		remove();
	}
	return result;
}
