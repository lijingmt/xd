#include <globals.h>
#include <gamelib/include/gamelib.h>
inherit WAP_BOOK;
void create(){
	name=object_name(this_object());
	name_cn="【妖】泥沼术";
	unit="本";
	picture=name;
	desc="召唤泥沼，覆盖敌人，降低对方的命中率\n";
	set_item_canDrop(1);
	set_item_canGet(1);
	set_item_canTrade(1);
	set_item_canSend(1);
	set_item_canStorage(1);
	value=50;
	skill_bname="nizhaoshu";
	level_limit=19;
	profe_read_limit="巫妖";
	need_yushi=10;
	need_money=400;
}
int read(){
	int result=::read();
	if(read_flag==0){
		remove();
	}
	return result;
}
