#include <globals.h>
#include <gamelib/include/gamelib.h>
inherit WAP_BOOK;
void create(){
	name=object_name(this_object());
	name_cn="【妖】嗜血狂暴二级";
	unit="本";
	picture=name;
	desc="使用狂暴之力增加自己的爆击率\n";
	set_item_canDrop(1);
	set_item_canGet(1);
	set_item_canTrade(1);
	set_item_canSend(1);
	set_item_canStorage(1);
	value=50;
	skill_bname="shixiekuangbao";
	level_limit=12;
	profe_read_limit="狂妖";
	beidong_level=2;
	need_yushi=10;
	need_money=100;
}
int read(){
	int result=::beidong_read();
	if(read_flag==0){
		remove();
	}
	return result;
}
