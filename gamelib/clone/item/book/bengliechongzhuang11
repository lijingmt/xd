#include <globals.h>
#include <gamelib/include/gamelib.h>
inherit WAP_BOOK;
void create(){
	name=object_name(this_object());
	name_cn="【妖】崩裂冲撞11级";
	unit="本";
	picture=name;
	desc="冲撞敌人身体，降低对方的防御能力\n";
	set_item_canDrop(1);
	set_item_canGet(1);
	set_item_canTrade(1);
	set_item_canSend(1);
	set_item_canStorage(1);
	value=50;
	skill_bname="bengliechongzhuang";
	level_limit=59;
	skill_level = 11; 
	profe_read_limit="狂妖";
}
int read(){
	int result=::read();
	if(read_flag==0){
		remove();
	}
	return result;
}
