#include <globals.h>
#include <gamelib/include/gamelib.h>
inherit WAP_WEAPON;
void create(){
	name=object_name(this_object());
	name_cn="【幽】血吼双刀";
	unit="对";
	picture=name;
	desc="\n";
	item_cur_dura=1700;
	item_dura=1700;
	set_item_type("double_weapon");
	set_item_weapon_type("jian");
	set_item_kind("double_main_weapon");
	set_item_canEquip(1);
	set_item_canDrop(1);
	set_item_canGet(1);
	set_item_canTrade(0);
	set_item_canSend(0);
	set_item_canStorage(1);
	set_attack_power(315);
	set_attack_power_limit(335);
	set_speed_power(4);
	set_item_canLevel(60);
	set_item_profeLimit("jianxian");
	set_item_profeLimit("kuangyao");
	if(query_item_kind()=="single_main_weapon")
		desc+="(主手)\n";
	else if(query_item_kind()=="single_other_weapon")
		desc+="(副手)\n";
	else if(query_item_kind()=="double_main_weapon")
		desc+="(双手)\n";
	set_str_add(34);
	set_dex_add(34);
	set_hitte_add(4);
	set_attack_add(36);
	set_weapon_attack_add(7);
	set_attack_huoyan_add(36);
	set_attack_dusu_add(36);
}
