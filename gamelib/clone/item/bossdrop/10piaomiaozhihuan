#include <globals.h>
#include <gamelib/include/gamelib.h>
inherit WAP_JEWELRY;
void create(){
	name=object_name(this_object());
	name_cn="【年】飘渺指环";
	unit="个";
	picture=name;
	desc="\n";
	item_cur_dura=10000;
	item_dura=10000;
	set_item_type("jewelry");
	set_item_kind("jewelry_ring");
	set_item_canEquip(1);
	set_item_canDrop(1);
	set_item_canGet(1);
	set_item_canTrade(0);
	set_item_canSend(0);
	set_item_canStorage(1);
	set_item_canLevel(10);
	set_item_profeLimit("zhuxian");
	set_item_profeLimit("yinggui");
	set_dex_add(5);
	set_dodge_add(1);
	set_doub_add(1);
	set_life_add(4);
	set_all_mofa_defend_add(5);
}
