#include <globals.h>
#include <gamelib/include/gamelib.h>
inherit WAP_JEWELRY;
void create(){
	name=object_name(this_object());
	name_cn="【年】细流项链";
	unit="条";
	picture=name;
	desc="\n";
	item_cur_dura=10000;
	item_dura=10000;
	set_item_type("jewelry");
	set_item_kind("jewelry_neck");
	set_item_canEquip(1);
	set_item_canDrop(1);
	set_item_canGet(1);
	set_item_canTrade(0);
	set_item_canSend(0);
	set_item_canStorage(1);
	set_item_canLevel(10);
	set_item_profeLimit("yushi");
	set_item_profeLimit("wuyao");
	set_think_add(4);
	set_hitte_add(1);
	set_lunck_add(2);
	set_mofa_add(6);
	set_rase_mofa_add(1);
}
