#include <globals.h>
#include <gamelib/include/gamelib.h>
inherit WAP_JEWELRY;
void create(){
	name=object_name(this_object());
	name_cn="【年】定山指环";
	unit="个";
	picture=name;
	desc="\n";
	item_cur_dura=10000;
	item_dura=10000;
	set_item_type("jewelry");
	set_item_kind("jewelry_ring");
	set_item_canEquip(1);
	set_item_canDrop(1);
	set_item_canGet(1);
	set_item_canTrade(0);
	set_item_canSend(0);
	set_item_canStorage(1);
	set_item_canLevel(50);
	set_item_profeLimit("jianxian");
	set_item_profeLimit("yushi");
	set_item_profeLimit("zhuxian");
	set_item_profeLimit("kuangyao");
	set_item_profeLimit("wuyao");
	set_item_profeLimit("yinggui");
	set_str_add(20);
	set_dodge_add(1);
	set_doub_add(2);
	set_life_add(10);
	set_rase_life_add(2);
	set_dex_add(30);
	set_think_add(30);
	set_hitte_add(3);
	set_lunck_add(40);
	set_bingshuang_defend_add(20);
	set_huoyan_defend_add(20);
	set_fengren_defend_add(20);
	set_dusu_defend_add(20);
}
