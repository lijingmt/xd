#include <globals.h>
#include <gamelib/include/gamelib.h>
inherit WAP_JEWELRY;
void create(){
	name=object_name(this_object());
	name_cn="【玄】天地手镯";
	unit="副";
	picture=name;
	desc="\n";
	item_cur_dura=10000;
	item_dura=10000;
	set_item_type("jewelry");
	set_item_kind("jewelry_bangle");
	set_item_canEquip(1);
	set_item_canDrop(1);
	set_item_canGet(1);
	set_item_canTrade(0);
	set_item_canSend(0);
	set_item_canStorage(1);
	set_item_canLevel(55);
	set_item_profeLimit("jianxian");
	set_item_profeLimit("yushi");
	set_item_profeLimit("zhuxian");
	set_item_profeLimit("kuangyao");
	set_item_profeLimit("wuyao");
	set_item_profeLimit("yinggui");
	set_str_add(35);
	set_dex_add(35);
	set_hitte_add(4);
	set_life_add(25);
	set_mofa_all_add(24);
	set_attack_dusu_add(24);
}
