#include <globals.h>
#include <gamelib/include/gamelib.h>
inherit WAP_ITEM;
void create(){
	name=object_name(this_object());
	name_cn="青铜门";
	unit="扇";
	desc="坚固度：6000\n";
	value=6000;
	amount=1;
	picture=name;
	set_item_canEquip(0);
	set_item_canDrop(1);
	set_item_canGet(1);
	set_item_canTrade(1);
	set_item_canSend(1);
	set_item_canStorage(1);
	set_item_type("door");
}
