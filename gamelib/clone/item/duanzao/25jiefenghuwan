#include <globals.h>
#include <gamelib/include/gamelib.h>
inherit WAP_ARMOR;
void create(){
	name=object_name(this_object());
	name_cn="【熔】结风护腕";
	unit="副";
	picture=name;
	desc="\n";
	item_cur_dura=550;
	item_dura=550;
	set_item_type("armor");
	set_item_kind("armor_waste");
	set_item_canEquip(1);
	set_item_canDrop(1);
	set_item_canGet(1);
	set_item_canTrade(1);
	set_item_canSend(1);
	set_item_canStorage(1);
	set_equip_defend(200);
	set_item_canLevel(25);
	set_item_profeLimit("jianxian");
	set_item_profeLimit("kuangyao");
	set_str_add(8);
	set_think_add(4);
	set_defend_add(1);
	set_mofa_add(5);
	set_bingshuang_defend_add(3);
	set_item_from("duanzao");
}
