#include <globals.h>
#include <gamelib/include/gamelib.h>
inherit WAP_ARMOR;
void create(){
	name=object_name(this_object());
	name_cn="【熔】仙人护手";
	unit="件";
	picture=name;
	desc="\n";
	item_cur_dura=1550;
	item_dura=1550;
	set_item_type("armor");
	set_item_kind("armor_hand");
	set_item_canEquip(1);
	set_item_canDrop(1);
	set_item_canGet(1);
	set_item_canTrade(1);
	set_item_canSend(1);
	set_item_canStorage(1);
	set_equip_defend(550);
	set_item_canLevel(53);
	set_item_profeLimit("jianxian");
	set_item_profeLimit("kuangyao");
	set_str_add(23);
	set_dex_add(16);
	set_dodge_add(2);
	set_hitte_add(2);
	set_defend_add(15);
	set_life_add(19);
	set_item_from("duanzao");
}
