#include <globals.h>
#include <gamelib/include/gamelib.h>
inherit WAP_FEED;
void create(){
	name=object_name(this_object());
	name_cn="五级狗粮";
	unit="包";
	picture=name;
	desc="狗吃后可永久增加7000点生命，5点力量，20点智力，5点敏捷\n";
	amount=1;
	value=100;
	set_item_canDrop(1);
	set_item_canGet(1);
	set_item_canTrade(1);
	set_item_canSend(1);
	set_item_canStorage(1);
	set_life_add(7000);
	set_str_add(5);
	set_think_add(20);
	set_dex_add(5);
	set_item_type("feed");
}