#include <globals.h>
#include <gamelib/include/gamelib.h>
inherit WAP_TRANSFER;
void create(){
	name=object_name(this_object());
	name_cn="【宝】飞天金钵";
	unit="个";
	picture=name;
	set_item_canDrop(1);
	set_item_canGet(1);
	set_item_canTrade(1);
	set_item_canSend(1);
	set_item_canStorage(1);
	set_item_level(10);
	desc="使用：可以将玩家传送到可用的任一传送阵\n";
}
