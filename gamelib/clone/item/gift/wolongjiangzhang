#include <globals.h>
#include <gamelib/include/gamelib.h>
inherit WAP_DECORATE;
void create(){
	name=object_name(this_object());
	name_cn="【宝】卧龙奖章";
	unit="个";
	picture=name;
	desc="以此纪念游戏问答活动中的胜利者！\n";
	item_cur_dura=1000;
	item_dura=1000;
	set_item_type("decorate");
	set_item_kind("decorate_thing");
	set_item_canEquip(1);
	set_item_canDrop(1);
	set_item_canGet(1);
	set_item_canTrade(0);
	set_item_canSend(0);
	set_item_canStorage(1);
	set_item_canLevel(25);
	set_item_profeLimit("jianxian");
	set_item_profeLimit("yushi");
	set_item_profeLimit("zhuxian");
	set_item_profeLimit("kuangyao");
	set_item_profeLimit("wuyao");
	set_item_profeLimit("yinggui");
	set_str_add(30);
	set_dex_add(30);
	set_think_add(31);
	set_item_from("gift");
}
