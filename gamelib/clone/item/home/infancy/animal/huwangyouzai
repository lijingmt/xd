#include <globals.h>
#include <gamelib/include/gamelib.h>
inherit WAP_INFANCY;
void create(){
	name=object_name(this_object());
	name_cn="虎王幼仔";
	unit="只";
	picture=name;
	desc="这家伙真漂亮，一定能长成一只虎王";
	amount=1;
	set_item_canDrop(1);
	set_item_canGet(1);
	set_item_canTrade(1);
	set_item_canSend(1);
	set_item_canStorage(1);
	value=100;
	set_item_type("home_infancy_animal");
	set_grownItem_path("home/grown/animal/huwang");
	set_harvest_desc("可得到虎王幼仔，金丝绸缎，兽王鳞");
	homeLevel_limit = 5;
}