#include <globals.h>
#include <gamelib/include/gamelib.h>
inherit WAP_INFANCY;
void create(){
	name=object_name(this_object());
	name_cn="原蹄兽幼仔";
	unit="只";
	picture=name;
	desc="这家伙真漂亮，一定能长成一只大原蹄兽";
	amount=1;
	set_item_canDrop(1);
	set_item_canGet(1);
	set_item_canTrade(1);
	set_item_canSend(1);
	set_item_canStorage(1);
	value=100;
	set_item_type("home_infancy_animal");
	set_grownItem_path("home/grown/animal/yuantishou");
	set_harvest_desc("可得到原蹄兽幼仔，碎布，轻皮，粗麻布，薄兽皮");
	homeLevel_limit = 1;
}