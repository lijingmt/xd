#include <globals.h>
#include <gamelib/include/gamelib.h>
inherit WAP_INFANCY;
void create(){
	name=object_name(this_object());
	name_cn="铁矿之源";
	unit="块";
	picture=name;
	desc="黑漆漆的一团，可以在你的家中汇聚出一处晶铁矿";
	amount=1;
	set_item_canDrop(1);
	set_item_canGet(1);
	set_item_canTrade(1);
	set_item_canSend(1);
	set_item_canStorage(1);
	value=100;
	set_item_type("home_infancy_ore");
	set_grownItem_path("home/grown/ore/jingtiekuang");
	set_harvest_desc("可得到铁矿之源，铁矿，晶铁石，白晶石");
	homeLevel_limit = 3;
}