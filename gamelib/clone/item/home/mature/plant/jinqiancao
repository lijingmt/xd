#include <globals.h>
#include <gamelib/include/gamelib.h>
inherit WAP_DANYAO;
void create(){
	name=object_name(this_object());
	name_cn="金钱草";
	unit="棵";
	picture="jinqiancao";
	desc="青绿的仙草，食用效果更佳，也可用作炼制丹药的材料\n";
	set_danyao_kind("attri_base");
	set_danyao_type("think");
	set_effect_value(100);
	set_danyao_timedelay(1800);
	set_item_canDrop(1);
	set_item_canGet(1);
	set_item_canTrade(1);
	set_item_canSend(1);
	set_item_canStorage(1);
	set_for_material("liandan");
}
