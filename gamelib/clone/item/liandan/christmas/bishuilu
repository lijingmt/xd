#include <globals.h>
#include <gamelib/include/gamelib.h>
inherit WAP_DANYAO;
void create(){
	name=object_name(this_object());
	name_cn="碧水露";
	desc="增加爆击率10%，持续30分钟\n";
	unit="瓶";
	picture=name;
	set_item_canDrop(1);
	set_item_canGet(1);
	set_item_canTrade(0);
	set_item_canSend(1);
	set_item_canStorage(1);
	set_danyao_kind("attri_vice");
	set_danyao_type("doub");
	set_effect_value(10);
	set_danyao_timedelay(1800);
	set_item_level(30);
}
