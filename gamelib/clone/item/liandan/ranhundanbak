#include <globals.h>
#include <gamelib/include/gamelib.h>
inherit WAP_DANYAO;
void create(){
	name=object_name(this_object());
	name_cn="燃魂丹";
	unit="颗";
	picture="ranhundan";
	desc="增加物理伤害40点，持续30分钟\n";
	set_danyao_kind("attri_attack");
	set_danyao_type("attack");
	set_effect_value(40);
	set_danyao_timedelay(1800);
	set_item_canDrop(0);
	set_item_canGet(1);
	set_item_canTrade(1);
	set_item_canSend(1);
	set_item_canStorage(1);
}
