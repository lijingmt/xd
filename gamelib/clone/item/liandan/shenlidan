#include <globals.h>
#include <gamelib/include/gamelib.h>
inherit WAP_DANYAO;
void create(){
	name=object_name(this_object());
	name_cn="神力丹";
	desc="增加力量20点，持续30分钟\n";
	unit="颗";
	picture=name;
	set_item_canDrop(1);
	set_item_canGet(1);
	set_item_canTrade(1);
	set_item_canSend(1);
	set_item_canStorage(1);
	set_danyao_kind("attri_base");
	set_danyao_type("str");
	set_effect_value(20);
	set_danyao_timedelay(1800);
	set_item_level(20);
}
