#include <globals.h>
#include <gamelib/include/gamelib.h>
inherit WAP_COMBINE_ITEM;
void create(){
	name=object_name(this_object());
	name_cn="白利齿";
	unit="颗";
	desc="稀有的制皮裁缝原料\n";
	value=8000;
	amount=1;
	picture=name;
	set_item_canEquip(0);
	set_item_canDrop(1);
	set_item_canGet(1);
	set_item_canTrade(1);
	set_item_canSend(1);
	set_item_canStorage(1);
	set_for_material("caifeng/zhijia");
}
