#include <globals.h>
#include <gamelib/include/gamelib.h>
inherit WAP_COMBINE_ITEM;
void create(){
	name=object_name(this_object());
	name_cn="冰晶石";
	unit="块";
	desc="珍贵的矿产资源\n";
	value=11000;
	amount=1;
	picture=name;
	set_item_canEquip(0);
	set_item_canDrop(1);
	set_item_canGet(1);
	set_item_canTrade(1);
	set_item_canSend(1);
	set_item_canStorage(1);
	set_for_material("duanzao");
}
