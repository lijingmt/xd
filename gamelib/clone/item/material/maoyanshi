#include <globals.h>
#include <gamelib/include/gamelib.h>
inherit WAP_COMBINE_ITEM;
void create(){
	name=object_name(this_object());
	name_cn="猫眼石";
	desc="矿藏中的宝石。\n";
	unit="颗";
	value=1;
	amount=1;
	//picture=name;
	value=1500;
	set_item_level(17);
	set_item_canEquip(0);
	set_item_canDrop(1);
	set_item_canGet(1);
	set_item_canTrade(1);
	set_item_canSend(1);
	set_item_canStorage(1);
	set_for_material("baoshi");
	set_add_luck(200);
}
