#include <globals.h>
#include <gamelib/include/gamelib.h>
inherit WAP_COMBINE_ITEM;
void create(){
	name=object_name(this_object());
	name_cn="铁矿石";
	desc="铁矿荣誉出品，可作锻造的原材料。\n";
	unit="块";
	amount=1;
	//picture=name;
	value=1;
	set_item_canEquip(0);
	set_item_canDrop(1);
	set_item_canGet(1);
	set_item_canTrade(1);
	set_item_canSend(1);
	set_item_canStorage(1);
	set_for_material("duanzao");
}
