#include <globals.h>
#include <gamelib/include/gamelib.h>
inherit WAP_COMBINE_ITEM;
void create(){
	name=object_name(this_object());
	name_cn="神女绣线";
	unit="根";
	desc="魔线，可以在你的裁缝和制甲中附加1000点幸运\n";
	value=16000;
	amount=1;
	picture=name;
	set_item_level(57);
	set_item_canEquip(0);
	set_item_canDrop(1);
	set_item_canGet(1);
	set_item_canTrade(1);
	set_item_canSend(1);
	set_item_canStorage(1);
	set_for_material("moxian");
	set_add_luck(1000);
}
