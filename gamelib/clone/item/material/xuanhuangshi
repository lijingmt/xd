#include <globals.h>
#include <gamelib/include/gamelib.h>
inherit WAP_COMBINE_ITEM;
void create(){
	name=object_name(this_object());
	name_cn="玄黄石";
	desc="矿藏中的宝石，散发着谈谈的黄光。\n";
	unit="颗";
	amount=1;
	//picture=name;
	value=1000;
	set_item_level(9);
	set_item_canEquip(0);
	set_item_canDrop(1);
	set_item_canGet(1);
	set_item_canTrade(1);
	set_item_canSend(1);
	set_item_canStorage(1);
	set_for_material("baoshi");
	set_add_luck(100);
}
