#include <globals.h>
#include <gamelib/include/gamelib.h>
inherit WAP_BOOK;
void create(){
	name=object_name(this_object());
	name_cn="【卷轴】冰冽匕首";
	unit="本";
	picture=name;
	desc="上面记载了冰冽匕的制作方法首\n";
	set_item_canDrop(1);
	set_item_canGet(1);
	set_item_canTrade(1);
	set_item_canSend(1);
	set_item_canStorage(1);
	set_peifang_kind("duanzao");
	set_peifang_type("weapon");
	peifang_id=134;
	level_limit=65;
	viceskill_level=195;
}
int read(){
	int result=::duanzao_read();
	if(read_flag==0){
		remove();
	}
	return result;
}
