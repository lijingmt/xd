#include <globals.h>
#include <gamelib/include/gamelib.h>
inherit WAP_BOOK;
void create(){
	name=object_name(this_object());
	name_cn="【配方】石化散";
	unit="本";
	picture=name;
	desc="上面记载了石化散的制造方法\n";
	set_item_canDrop(1);
	set_item_canGet(1);
	set_item_canTrade(1);
	set_item_canSend(1);
	set_item_canStorage(1);
	set_peifang_kind("liandan");
	set_peifang_type("attri_defend");
	peifang_id=22;
	level_limit=10;
	viceskill_level=40;
}
int read(){
	int result=::liandan_read();
	if(read_flag==0){
		remove();
	}
	return result;
}
