#include <globals.h>
#include <gamelib/include/gamelib.h>
inherit WAP_BOOK;
void create(){
	name=object_name(this_object());
	name_cn="【图纸】苍狼长裤";
	unit="本";
	picture=name;
	desc="上面记载了苍狼长裤的制造方法\n";
	set_item_canDrop(1);
	set_item_canGet(1);
	set_item_canTrade(1);
	set_item_canSend(1);
	set_item_canStorage(1);
	set_peifang_kind("zhijia");
	set_peifang_type("thou");
	peifang_id=56;
	level_limit=42;
	viceskill_level=120;
}
int read(){
	int result=::zhijia_read();
	if(read_flag==0){
		remove();
	}
	return result;
}
