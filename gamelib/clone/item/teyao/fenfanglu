#include <globals.h>
#include <gamelib/include/gamelib.h>
inherit WAP_DANYAO;
void create(){
	name=object_name(this_object());
	name_cn="芬芳露";
	unit="瓶";
	desc="增加打怪获得经验500%， 持续30分钟，下线后效果不消失\n";
	set_danyao_kind("te_exp");
	set_danyao_type("exp");
	set_effect_value(500);
	set_danyao_timedelay(1800);
	set_item_canDrop(1);
	set_item_canGet(1);
	set_item_canTrade(1);
	set_item_canSend(1);
	set_item_canStorage(1);
	set_item_level(1);
}
