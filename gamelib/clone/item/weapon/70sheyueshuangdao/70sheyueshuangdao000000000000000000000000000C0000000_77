#include <globals.h>
#include <gamelib/include/gamelib.h>
inherit WAP_WEAPON;
void create(){
	name=object_name(this_object());
	name_cn="麝月双刀";
	unit="对";
	picture="70sheyueshuangdao";
	desc="\n";
	item_cur_dura=1900;
	item_dura=1900;
	set_item_type("double_weapon");
	set_item_weapon_type("jian");
	set_item_kind("double_main_weapon");
	set_item_canEquip(1);
	set_item_canDrop(1);
	set_item_canGet(1);
	set_item_canTrade(1);
	set_item_canSend(1);
	set_item_canStorage(1);
    set_attack_power(358);
	set_attack_power_limit(385);
	set_speed_power(4);
    set_item_canLevel(77);
	set_item_profeLimit("jianxian");
	set_item_profeLimit("kuangyao");
	set_aocao_max("red",2);
	set_aocao_max("yellow",1);
	if(query_item_kind()=="single_main_weapon")
		desc+="(主手)\n";
	else if(query_item_kind()=="single_other_weapon")
		desc+="(副手)\n";
	else if(query_item_kind()=="double_main_weapon")
		desc+="(双手)\n";
    set_item_rareLevel(1);
    set_attack_fengren_add(38);
    name_cn=query_rare_level()+name_cn;
}
