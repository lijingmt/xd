#include <globals.h>
#include <gamelib/include/gamelib.h>
inherit WAP_YUSHI;
void create(){
	name=object_name(this_object());
	name_cn="【玉】碧銮玉";
	desc="高级的玉石\n";
	unit="块";
	picture=name;
	set_item_canDrop(0);
	set_item_canGet(1);
	set_item_canTrade(1);
	set_item_canSend(1);
	set_item_canStorage(1);
	set_yushi_rarelevel(4);
	set_yushi_value(1000);
}
