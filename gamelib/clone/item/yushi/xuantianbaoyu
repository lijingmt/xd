#include <globals.h>
#include <gamelib/include/gamelib.h>
inherit WAP_YUSHI;
void create(){
	name=object_name(this_object());
	name_cn="【玉】玄天宝玉";
	desc="顶级的玉石\n";
	unit="块";
	picture=name;
	set_item_canDrop(0);
	set_item_canGet(1);
	set_item_canTrade(1);
	set_item_canSend(1);
	set_item_canStorage(1);
	set_yushi_rarelevel(5);
	set_yushi_value(10000);
}
