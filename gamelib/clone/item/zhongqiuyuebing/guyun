#include <globals.h>
#include <gamelib/include/gamelib.h>
inherit WAP_DANYAO;
void create(){
	name=object_name(this_object());
	name_cn="古韵月饼";
	unit="个";
	picture="guyun";
	desc="永久增加幸运10点\n";
	set_danyao_kind("te_luck");
	set_danyao_type("luck");
	set_effect_value(10);
	set_danyao_timedelay(0);
	set_item_canDrop(1);
	set_item_canGet(1);
	set_item_canTrade(1);
	set_item_canSend(1);
	set_item_canStorage(1);
}
