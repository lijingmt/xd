#include <globals.h>
#include <gamelib/include/gamelib.h>
inherit GAMELIB_NPC;
void create(){
	name=object_name(this_object());
	_boss=1;
	_flushtime=3600;
	name_cn="土蛟龙";
	desc="五行蛟龙王之一，掌管土性龙力\n";
	set_raceId("third");
	set_profeId("humanlike");
	picture="tujiaolong";
	_npcLevel=40;
	set_base_dex(150);
	set_base_str(200);
	set_base_life(160000);
	this_object()->query_life_max();
	set_base_baoji(5);
	set_base_hitte(85);
	set_base_dodge(5);
	boss_skills=(["60/60":"b_jianyaci","180/180":"b_diliezhenji",]);
	setup_npc();
	set_heart_beat(1);
}
string query_words(){
	string s = ::query_words();
	s += TASKD->query_words(this_player(),this_object());
	return s;
}
string query_links(void|int count){
	return ::query_links(count);
}
void fight_die(){
	::fight_die();
}
