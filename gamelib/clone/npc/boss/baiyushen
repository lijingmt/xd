	name=object_name(this_object());
	_boss=1;
	_flushtime=36000;
	name_cn="°×ÓðÉñ";
	desc="¾Þ´óÒø°×Óð³á£¬×¯ÑÏËàÄÂÓÖÎÂÈáµÄÑÛÉñ£¬ÁîÈË×ÅÃÔ.\n";
