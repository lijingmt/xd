#include <globals.h>
#include <gamelib/include/gamelib.h>
inherit GAMELIB_NPC;
void create(){
	name=object_name(this_object());
	_boss=1;
	_flushtime=3600;
	name_cn="妖鸟鬼车";
	desc="拥有九个巨大头颅的妖鸟，鬼王的爱宠\n";
	set_raceId("third");
	set_profeId("bird");
	picture="yaoniaoguiche";
	_npcLevel=34;
	set_base_think(100);
	set_base_dex(200);
	set_base_str(100);
	set_base_life(60000);
	this_object()->query_life_max();
	set_base_baoji(10);
	set_base_hitte(100);
	set_base_dodge(10);
	boss_skills=(["20/60":"b_qianhuiyao","10/10":"b_chongtiantuji","60/180":"b_huoyanliuxingyu",]);
	setup_npc();
	set_heart_beat(1);
}
string query_words(){
	string s = ::query_words();
	s += TASKD->query_words(this_player(),this_object());
	return s;
}
string query_links(void|int count){
	return ::query_links(count);
}
void fight_die(){
	::fight_die();
}
