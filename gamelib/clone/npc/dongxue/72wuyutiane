#include <gamelib/include/gamelib.h>
inherit GAMELIB_NPC;
void create(){
	name=object_name(this_object());
	desc="\n";
	set_raceId("third");
	set_profeId("bird");
	sex="female";
	gender="´Æ";
	pronoun="Ëü";
	picture="bird_female";
	_npcLevel=72;
	name_cn="ÎÞÓðÌì¶ì";
	_meritocrat=1;
	setup_npc();
}
string query_words(){
	string s = ::query_words();
	s += TASKD->query_words(this_player(),this_object());
	return s;
}
string query_links(void|int count){
	return ::query_links(count);
}
void fight_die(){
	::fight_die();
}
