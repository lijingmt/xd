#include <globals.h>
#include <gamelib/include/gamelib.h>
inherit GAMELIB_NPC;
void create(){
	name=object_name(this_object());
	desc="墓地的特有生物，以吃食腐肉为生。\n";
	set_raceId("third");
	set_profeId("beast");
	sex="male";
	gender="雄";
	pronoun="它";
	picture="beast_male";
	_npcLevel=20;
	name_cn="荒野豺狼";
	setup_npc();
}
string query_words(){
	string s = ::query_words();
	s += TASKD->query_words(this_player(),this_object());
	return s;
}
string query_links(void|int count){
	return ::query_links(count);
}
void fight_die(){
	::fight_die();
}
