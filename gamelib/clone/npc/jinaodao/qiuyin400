#include <globals.h>
#include <gamelib/include/gamelib.h>
inherit GAMELIB_NPC;
void create(){
	name=object_name(this_object());
	desc="曲鳝成精\n";
	set_raceId("monst");
	set_profeId("humanlike");
	sex="male";
	gender="男";
	pronoun="他";
	picture=name;
	_npcLevel=400;
	name_cn="丘引";
	setup_npc();
}
string query_words(){
	string s = ::query_words();
	s += "[银色徽记兑换:hj_equip_exchange]\n";
	s += "[火月签兑换:hyq_exchange]\n";
	s += TASKD->query_words(this_player(),this_object());
	return s;
}
string query_links(void|int count){
	return ::query_links(count);
}
void fight_die(){
	::fight_die();
}
