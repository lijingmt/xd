#include <globals.h>
#include <gamelib/include/gamelib.h>
inherit GAMELIB_NPC;
void create(){
	name=object_name(this_object());
	desc="吃食腐肉的蝙蝠怪，有极强的攻击性。\n";
	set_raceId("third");
	set_profeId("beast");
	sex="female";
	gender="雌";
	pronoun="它";
	picture="beast_female";
	_npcLevel=5;
	name_cn="食肉蝙蝠";
	setup_npc();
}
string query_words(){
	string s = ::query_words();
	s += TASKD->query_words(this_player(),this_object());
	return s;
}
string query_links(void|int count){
	return ::query_links(count);
}
void fight_die(){
	::fight_die();
}
