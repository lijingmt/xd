#include <globals.h>
#include <gamelib/include/gamelib.h>
inherit GAMELIB_NPC;
void create(){
	name=object_name(this_object());
	desc="\n";
	set_raceId("third");
	set_profeId("bird");
	sex="male";
	gender="ÐÛ";
	pronoun="Ëü";
	picture="bird_male";
	_npcLevel=53;
	name_cn="Õñ³á°×º×";
