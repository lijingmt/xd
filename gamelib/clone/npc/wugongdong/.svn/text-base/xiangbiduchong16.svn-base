#include <globals.h>
#include <gamelib/include/gamelib.h>
inherit GAMELIB_NPC;
void create(){
	name=object_name(this_object());
	desc="拥有长鼻的甲虫，可以从鼻子内喷射出浓烈的毒液。\n";
	set_raceId("third");
	set_profeId("bugs");
	sex="female";
	gender="雌";
	pronoun="它";
	picture="bugs_female";
	_npcLevel=16;
	name_cn="象鼻毒虫";
	setup_npc();
}
string query_words(){
	string s = ::query_words();
	s += TASKD->query_words(this_player(),this_object());
	return s;
}
string query_links(void|int count){
	return ::query_links(count);
}
void fight_die(){
	::fight_die();
}
