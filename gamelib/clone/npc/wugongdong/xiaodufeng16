#include <globals.h>
#include <gamelib/include/gamelib.h>
inherit GAMELIB_NPC;
void create(){
	name=object_name(this_object());
	desc="巨大的毒蜂，成群结队的在黑暗的洞穴内徘徊。\n";
	set_raceId("third");
	set_profeId("bugs");
	sex="female";
	gender="雌";
	pronoun="它";
	picture="bugs_female";
	_npcLevel=16;
	name_cn="黑翅毒蜂";
	setup_npc();
}
string query_words(){
	string s = ::query_words();
	s += TASKD->query_words(this_player(),this_object());
	return s;
}
string query_links(void|int count){
	return ::query_links(count);
}
void fight_die(){
	::fight_die();
}
