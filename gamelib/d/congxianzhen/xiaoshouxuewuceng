	add_items(({ROOT "/gamelib/clone/npc/kunlunshan/yaodao5"}));
	add_items(({ROOT "/gamelib/clone/npc/kunlunshan/tulou5"}));
	add_items(({ROOT "/gamelib/clone/npc/kunlunshan/wunaojiangshi5"}));
	add_items(({ROOT "/gamelib/clone/npc/kunlunshan/shiroubianfu5"}));
}
