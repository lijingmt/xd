#include <globals.h>
#include <gamelib/include/gamelib.h>
inherit WAP_ROOM;
string room_race="monst";
static int room_level=165;
void create(){
	name=object_name(this_object());
	name_cn="地窖十八层";
	desc="\n";
	exits["south"]=ROOT "/gamelib/d/fushoushan/dijiaoshijiuceng";
	exits["north"]=ROOT "/gamelib/d/fushoushan/dijiaoshiqiceng";
	desc+="[走出洞穴:qge74hye "+"fushoushan/dijiaoyiceng"+"]\n";
	add_items(({ROOT "/gamelib/clone/npc/fushoushan/nongfu19"}));
	add_items(({ROOT "/gamelib/clone/npc/fushoushan/funv19"}));
	add_items(({ROOT "/gamelib/clone/npc/fushoushan/qiaofu19"}));
	add_items(({ROOT "/gamelib/clone/npc/fushoushan/fushoucunming19"}));
	add_items(({ROOT "/gamelib/clone/npc/fushoushan/fushoushouwei19"}));
	add_items(({ROOT "/gamelib/clone/npc/fushoushan/xiaofan19"}));
}
