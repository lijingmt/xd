#include <globals.h>
#include <gamelib/include/gamelib.h>
inherit WAP_ROOM;
string room_race="monst";
static int room_level=196;
void create(){
	name=object_name(this_object());
	name_cn="福寿农场二十层";
	desc="\n";
	exits["south"]=ROOT "/gamelib/d/fushoushan/fushounongchangshijiuceng";
	desc+="[走出洞穴:qge74hye "+"fushoushan/fushounongchangyiceng"+"]\n";
	add_items(({ROOT "/gamelib/clone/npc/fushouzhuangyuan/guonong25"}));
	add_items(({ROOT "/gamelib/clone/npc/fushouzhuangyuan/yuanlinhuajiang25"}));
	add_items(({ROOT "/gamelib/clone/npc/fushouzhuangyuan/caichanv25"}));
	add_items(({ROOT "/gamelib/clone/npc/fushouzhuangyuan/genghuangzhe25"}));
	add_items(({ROOT "/gamelib/clone/npc/fushouzhuangyuan/zhuangyuanbaobiao25"}));
	add_items(({ROOT "/gamelib/clone/npc/fushouzhuangyuan/nongchangjiangong25"}));
}
