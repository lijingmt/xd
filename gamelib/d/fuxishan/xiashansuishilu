	add_items(({ROOT "/gamelib/clone/npc/fuxishan/49fuxishanlinshouwei"}));
	add_items(({ROOT "/gamelib/clone/npc/fuxishan/49fuxibianyishaobing"}));
	add_items(({ROOT "/gamelib/clone/npc/fuxishan/49shenshankuxiuzhe"}));
	add_items(({ROOT "/gamelib/clone/npc/fuxishan/49fuxishanzei"}));
	add_items(({ROOT "/gamelib/clone/npc/fuxishan/49fuxihuwei"}));
}
