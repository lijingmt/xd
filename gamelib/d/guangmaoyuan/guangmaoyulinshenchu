#include <globals.h>
#include <gamelib/include/gamelib.h>
inherit WAP_ROOM;
string room_race="third";
static int room_level=1198;
void create(){
	name=object_name(this_object());
	name_cn="广袤雨林深处";
	desc="\n";
	exits["south"]=ROOT "/gamelib/d/guangmaoyuan/yuwulin";
	exits["west"]=ROOT "/gamelib/d/guangmaoyuan/guangmaoyulinqianchu";
	exits["north"]=ROOT "/gamelib/d/guangmaoyuan/zhangqilin";
	add_items(({ROOT "/gamelib/clone/npc/guangmaoyuan/50yinlujingling"}));
	add_items(({ROOT "/gamelib/clone/npc/guangmaoyuan/50yulinshouwei"}));
	add_items(({ROOT "/gamelib/clone/npc/guangmaoyuan/50senlinlieshou"}));
	add_items(({ROOT "/gamelib/clone/npc/guangmaoyuan/50yulinmenghu"}));
	add_items(({ROOT "/gamelib/clone/npc/guangmaoyuan/50yulinjushe"}));
	add_items(({ROOT "/gamelib/clone/npc/guangmaoyuan/50yulinkuangmo"}));
}
