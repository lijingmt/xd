string query_links()                                                                                                         
{   
	object player=this_player();
	string tmp="";
	if(player->query_raceId()==room_race&&player->query_level()>=10)
		tmp+="[œ…’Û°æ”Ò–Èπ¨°ø:qge74hye kunlunshan/xianzhenxuyugong]\n";
	return tmp;
}
