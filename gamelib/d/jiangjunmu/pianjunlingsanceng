#include <globals.h>
#include <gamelib/include/gamelib.h>
inherit WAP_ROOM;
string room_race="human";
static int room_level=285;
void create(){
	name=object_name(this_object());
	name_cn="偏军陵三层";
	desc="\n";
	exits["south"]=ROOT "/gamelib/d/jiangjunmu/pianjunlingerceng";
	exits["north"]=ROOT "/gamelib/d/jiangjunmu/pianjunlingsiceng";
	desc+="[走出洞穴:qge74hye "+"jiangjunmu/pianjunlingyiceng"+"]\n";
	add_items(({ROOT "/gamelib/clone/npc/jiangjunmu/guilingshibing21"}));
	add_items(({ROOT "/gamelib/clone/npc/jiangjunmu/guilinggongbing21"}));
	add_items(({ROOT "/gamelib/clone/npc/jiangjunmu/kulouqibing21"}));
	add_items(({ROOT "/gamelib/clone/npc/jiangjunmu/kulouzhanma21"}));
}
