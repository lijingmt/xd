#include <globals.h>
#include <gamelib/include/gamelib.h>
inherit WAP_ROOM;
string room_race="monst";
static int room_level=6;
void create(){
	name=object_name(this_object());
	name_cn="出村土路";
	desc="\n";
	exits["south"]=ROOT "/gamelib/d/jinaodao/huangmo";
	exits["north"]=ROOT "/gamelib/d/jinaodao/yuhuacunguangchang";
	add_items(({ROOT "/gamelib/clone/npc/jinaodao/shachong1"}));
	add_items(({ROOT "/gamelib/clone/npc/jinaodao/youhun1"}));
	add_items(({ROOT "/gamelib/clone/npc/jinaodao/tianxianjiaocike1"}));
	add_items(({ROOT "/gamelib/clone/npc/jinaodao/duwuhuoshe1"}));
	add_items(({ROOT "/gamelib/clone/npc/jinaodao/huangshaxiyi1"}));
	add_items(({ROOT "/gamelib/clone/npc/jinaodao/yeying1"}));
}
