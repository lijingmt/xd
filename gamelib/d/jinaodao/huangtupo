	name=object_name(this_object());
	name_cn="ปฦอมฦย";
	desc="\n";
	exits["east"]=ROOT "/gamelib/d/jinaodao/huangshayuanye";
	exits["south"]=ROOT "/gamelib/d/jinaodao/qianshakeng";
	exits["west"]=ROOT "/gamelib/d/jinaodao/huangshagang";
	exits["north"]=ROOT "/gamelib/d/jinaodao/feituidu";
	add_items(({ROOT "/gamelib/clone/npc/jinaodao/shachong2"}));
	add_items(({ROOT "/gamelib/clone/npc/jinaodao/youhun2"}));
	add_items(({ROOT "/gamelib/clone/npc/jinaodao/tianxianjiaocike2"}));
	add_items(({ROOT "/gamelib/clone/npc/jinaodao/duwuhuoshe2"}));
	add_items(({ROOT "/gamelib/clone/npc/jinaodao/huangshaxiyi2"}));
	add_items(({ROOT "/gamelib/clone/npc/jinaodao/yeying2"}));
}
