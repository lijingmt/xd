#include <globals.h>
#include <gamelib/include/gamelib.h>
inherit WAP_ROOM;
string room_race="human";
static int room_level=50;
void create(){
	name=object_name(this_object());
	name_cn="黑色岩穴四层";
	desc="\n";
	exits["south"]=ROOT "/gamelib/d/kunlunshan/heiseyanxuewuceng";
	exits["north"]=ROOT "/gamelib/d/kunlunshan/heiseyanxuesanceng";
	desc+="[走出洞穴:qge74hye "+"kunlunshan/heiseyanxuerukou"+"]\n";
	add_items(({ROOT "/gamelib/clone/npc/kunlunshan/qinyuan9"}));
	add_items(({ROOT "/gamelib/clone/npc/kunlunshan/shuanglang9"}));
	add_items(({ROOT "/gamelib/clone/npc/kunlunshan/yaodao9"}));
	add_items(({ROOT "/gamelib/clone/npc/kunlunshan/tulou9"}));
	add_items(({ROOT "/gamelib/clone/npc/kunlunshan/wunaojiangshi9"}));
	add_items(({ROOT "/gamelib/clone/npc/kunlunshan/shiroubianfu9"}));
}
