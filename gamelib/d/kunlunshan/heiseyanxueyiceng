#include <globals.h>
#include <gamelib/include/gamelib.h>
inherit WAP_ROOM;
string room_race="human";
static int room_level=47;
void create(){
	name=object_name(this_object());
	name_cn="黑色岩穴一层";
	desc="\n";
	exits["south"]=ROOT "/gamelib/d/kunlunshan/heiseyanxueerceng";
	exits["north"]=ROOT "/gamelib/d/kunlunshan/heiseyanxuerukou";
	desc+="[走出洞穴:qge74hye "+"kunlunshan/heiseyanxuerukou"+"]\n";
	add_items(({ROOT "/gamelib/clone/npc/kunlunshan/qinyuan7"}));
	add_items(({ROOT "/gamelib/clone/npc/kunlunshan/shuanglang7"}));
	add_items(({ROOT "/gamelib/clone/npc/kunlunshan/yaodao7"}));
	add_items(({ROOT "/gamelib/clone/npc/kunlunshan/tulou7"}));
	add_items(({ROOT "/gamelib/clone/npc/kunlunshan/wunaojiangshi7"}));
	add_items(({ROOT "/gamelib/clone/npc/kunlunshan/shiroubianfu7"}));
}
