#include <globals.h>
#include <gamelib/include/gamelib.h>
inherit WAP_ROOM;
string room_race="third";
static int room_level=355;
void create(){
	name=object_name(this_object());
	name_cn="荷花迷宫十一层";
	desc="\n";
	exits["south"]=ROOT "/gamelib/d/liangjinghu/hehuamigong10";
	exits["north"]=ROOT "/gamelib/d/liangjinghu/hehuamigong12";
	desc+="[走出洞穴:qge74hye "+"liangjinghu/hehuamigong1"+"]\n";
	add_items(({ROOT "/gamelib/clone/npc/liangjinghu/shuixichong27"}));
	add_items(({ROOT "/gamelib/clone/npc/liangjinghu/xiaoheyao27"}));
	add_items(({ROOT "/gamelib/clone/npc/liangjinghu/shiroufuping27"}));
	add_items(({ROOT "/gamelib/clone/npc/liangjinghu/hehuanv27"}));
	add_items(({ROOT "/gamelib/clone/npc/liangjinghu/baiyanlianpeng27"}));
	add_items(({ROOT "/gamelib/clone/npc/liangjinghu/duouguai27"}));
}
