	exits["south"]=ROOT "/gamelib/d/liangjinghu/hehuamigong13";
	exits["north"]=ROOT "/gamelib/d/liangjinghu/hehuamigong15";
	desc+="[走出洞穴:qge74hye "+"liangjinghu/hehuamigong1"+"]\n";
	add_items(({ROOT "/gamelib/clone/npc/liangjinghu/shuixichong28"}));
	add_items(({ROOT "/gamelib/clone/npc/liangjinghu/xiaoheyao28"}));
	add_items(({ROOT "/gamelib/clone/npc/liangjinghu/shiroufuping28"}));
	add_items(({ROOT "/gamelib/clone/npc/liangjinghu/hehuanv28"}));
	add_items(({ROOT "/gamelib/clone/npc/liangjinghu/baiyanlianpeng28"}));
	add_items(({ROOT "/gamelib/clone/npc/liangjinghu/duouguai28"}));
}
