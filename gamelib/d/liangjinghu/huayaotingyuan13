	add_items(({ROOT "/gamelib/clone/npc/liangjinghu/meiguiteng28"}));
	add_items(({ROOT "/gamelib/clone/npc/liangjinghu/taohuajingguai28"}));
	add_items(({ROOT "/gamelib/clone/npc/liangjinghu/guihudie28"}));
	add_items(({ROOT "/gamelib/clone/npc/liangjinghu/duhuapopo28"}));
	add_items(({ROOT "/gamelib/clone/npc/liangjinghu/hongyanmeifu28"}));
	add_items(({ROOT "/gamelib/clone/npc/liangjinghu/jiaomeihuayao28"}));
}
