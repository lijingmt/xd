	add_items(({ROOT "/gamelib/clone/npc/liangjinghu/meiguiteng30"}));
	add_items(({ROOT "/gamelib/clone/npc/liangjinghu/taohuajingguai30"}));
	add_items(({ROOT "/gamelib/clone/npc/liangjinghu/guihudie30"}));
	add_items(({ROOT "/gamelib/clone/npc/liangjinghu/duhuapopo30"}));
	add_items(({ROOT "/gamelib/clone/npc/liangjinghu/hongyanmeifu30"}));
	add_items(({ROOT "/gamelib/clone/npc/liangjinghu/jiaomeihuayao30"}));
