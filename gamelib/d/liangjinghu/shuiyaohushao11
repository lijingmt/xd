#include <globals.h>
#include <gamelib/include/gamelib.h>
inherit WAP_ROOM;
string room_race="third";
static int room_level=409;
void create(){
	name=object_name(this_object());
	name_cn="水妖湖穴十一层";
	desc="\n";
	exits["south"]=ROOT "/gamelib/d/liangjinghu/shuiyaohushao12";
	exits["north"]=ROOT "/gamelib/d/liangjinghu/shuiyaohushao10";
	desc+="[走出洞穴:qge74hye "+"liangjinghu/shuiyaohushao1"+"]\n";
	add_items(({ROOT "/gamelib/clone/npc/liangjinghu/juqianlongxia27"}));
	add_items(({ROOT "/gamelib/clone/npc/liangjinghu/bazushuiyao27"}));
	add_items(({ROOT "/gamelib/clone/npc/liangjinghu/nianyujing27"}));
	add_items(({ROOT "/gamelib/clone/npc/liangjinghu/tiejiawugui27"}));
	add_items(({ROOT "/gamelib/clone/npc/liangjinghu/chanrenshuicao27"}));
	add_items(({ROOT "/gamelib/clone/npc/liangjinghu/liuzhuae27"}));
}
