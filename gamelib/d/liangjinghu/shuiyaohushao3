#include <globals.h>
#include <gamelib/include/gamelib.h>
inherit WAP_ROOM;
string room_race="third";
static int room_level=401;
void create(){
	name=object_name(this_object());
	name_cn="水妖湖穴三层";
	desc="\n";
	exits["south"]=ROOT "/gamelib/d/liangjinghu/shuiyaohushao4";
	exits["north"]=ROOT "/gamelib/d/liangjinghu/shuiyaohushao2";
	desc+="[走出洞穴:qge74hye "+"liangjinghu/shuiyaohushao1"+"]\n";
	add_items(({ROOT "/gamelib/clone/npc/liangjinghu/juqianlongxia25"}));
	add_items(({ROOT "/gamelib/clone/npc/liangjinghu/bazushuiyao25"}));
	add_items(({ROOT "/gamelib/clone/npc/liangjinghu/nianyujing25"}));
	add_items(({ROOT "/gamelib/clone/npc/liangjinghu/tiejiawugui25"}));
	add_items(({ROOT "/gamelib/clone/npc/liangjinghu/chanrenshuicao25"}));
	add_items(({ROOT "/gamelib/clone/npc/liangjinghu/liuzhuae25"}));
}
