	exits["north"]=ROOT "/gamelib/d/liangjinghu/shuiyaohushao4";
	desc+="[走出洞穴:qge74hye "+"liangjinghu/shuiyaohushao1"+"]\n";
	add_items(({ROOT "/gamelib/clone/npc/liangjinghu/juqianlongxia26"}));
	add_items(({ROOT "/gamelib/clone/npc/liangjinghu/bazushuiyao26"}));
	add_items(({ROOT "/gamelib/clone/npc/liangjinghu/nianyujing26"}));
	add_items(({ROOT "/gamelib/clone/npc/liangjinghu/tiejiawugui26"}));
	add_items(({ROOT "/gamelib/clone/npc/liangjinghu/chanrenshuicao26"}));
	add_items(({ROOT "/gamelib/clone/npc/liangjinghu/liuzhuae26"}));
}
