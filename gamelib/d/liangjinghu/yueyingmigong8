	add_items(({ROOT "/gamelib/clone/npc/liangjinghu/baiying26"}));
	add_items(({ROOT "/gamelib/clone/npc/liangjinghu/yingzicike26"}));
	add_items(({ROOT "/gamelib/clone/npc/liangjinghu/yueguangshou26"}));
	add_items(({ROOT "/gamelib/clone/npc/liangjinghu/yinmaishuishe26"}));
	add_items(({ROOT "/gamelib/clone/npc/liangjinghu/wuyingqingting26"}));
