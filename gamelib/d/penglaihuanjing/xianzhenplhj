}
string query_links() 
{   
	object player=this_player();
	string tmp="";
	if(player->query_raceId()==room_race&&player->query_level()>=58)
		tmp+="[œ…’Û°æ≈Ó¿≥œ…æ≥°ø:qge74hye plxianjing/xianzhenplxianjing]\n";
	return tmp;
}
