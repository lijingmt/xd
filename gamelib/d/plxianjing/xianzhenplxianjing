#include <globals.h>
#include <gamelib/include/gamelib.h>
inherit WAP_ROOM;
string room_race="human";
static int room_level=1206;
void create(){
	name=object_name(this_object());
	name_cn="œ…’Û°æ≈Ó¿≥œ…æ≥°ø";
	desc="\n";
	exits["south"]=ROOT "/gamelib/d/plxianjing/lingxianzhen";
