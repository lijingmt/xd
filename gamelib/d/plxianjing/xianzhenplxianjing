int is_peaceful()
{
	return 1;
}
string query_links() 
{   
	object player=this_player();
	string tmp="";
	if(player->query_raceId()==room_race&&player->query_level()>=50)
		tmp+="[œ…’Û°æƒ¡“∞°ø:qge74hye muye/xianzhenmuye]\n";
