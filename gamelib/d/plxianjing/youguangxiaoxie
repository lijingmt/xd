	add_items(({ROOT "/gamelib/clone/npc/plxianjing/55ningleishuiyao"}));
	add_items(({ROOT "/gamelib/clone/npc/plxianjing/55piaomiaoxianling"}));
	add_items(({ROOT "/gamelib/clone/npc/plxianjing/55xukongyaopo"}));
	add_items(({ROOT "/gamelib/clone/npc/plxianjing/55zhenchibaihe"}));
	add_items(({ROOT "/gamelib/clone/npc/plxianjing/55yaomeishejing"}));
}
