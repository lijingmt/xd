#include <globals.h>
#include <gamelib/include/gamelib.h>
inherit WAP_ROOM;
string room_race="human";
static int room_level=169;
void create(){
	name=object_name(this_object());
	name_cn="飞云洞十五层";
	desc="\n";
	exits["south"]=ROOT "/gamelib/d/shierxianjing/feiyundongshisiceng";
	exits["north"]=ROOT "/gamelib/d/shierxianjing/feiyundongshiliuceng";
	desc+="[走出洞穴:qge74hye "+"shierxianjing/feiyundongyiceng"+"]\n";
	add_items(({ROOT "/gamelib/clone/npc/shierxianshan/qingyunshou18"}));
	add_items(({ROOT "/gamelib/clone/npc/shierxianshan/weilong18"}));
	add_items(({ROOT "/gamelib/clone/npc/shierxianshan/leiniao18"}));
	add_items(({ROOT "/gamelib/clone/npc/shierxianshan/linggui18"}));
}
