#include <globals.h>
#include <gamelib/include/gamelib.h>
inherit WAP_ROOM;
string room_race="human";
static int room_level=217;
void create(){
	name=object_name(this_object());
	name_cn="落伽洞十二层";
	desc="\n";
	exits["south"]=ROOT "/gamelib/d/shierxianjing/luojiadongshiyiceng";
	exits["north"]=ROOT "/gamelib/d/shierxianjing/luojiadongshisanceng";
	desc+="[走出洞穴:qge74hye "+"shierxianjing/luojiadongyiceng"+"]\n";
	add_items(({ROOT "/gamelib/clone/npc/shierxianshan/juyanguai22"}));
	add_items(({ROOT "/gamelib/clone/npc/shierxianshan/baixushuyao22"}));
	add_items(({ROOT "/gamelib/clone/npc/shierxianshan/xiaopanlong22"}));
	add_items(({ROOT "/gamelib/clone/npc/shierxianshan/huoyunshou22"}));
}
