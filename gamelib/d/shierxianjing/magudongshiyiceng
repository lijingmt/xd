#include <globals.h>
#include <gamelib/include/gamelib.h>
inherit WAP_ROOM;
string room_race="human";
static int room_level=191;
void create(){
	name=object_name(this_object());
	name_cn="麻姑洞十一层";
	desc="\n";
	exits["south"]=ROOT "/gamelib/d/shierxianjing/magudongshiceng";
	exits["north"]=ROOT "/gamelib/d/shierxianjing/magudongshierceng";
	desc+="[走出洞穴:qge74hye "+"shierxianjing/magudongyiceng"+"]\n";
	add_items(({ROOT "/gamelib/clone/npc/shierxianshan/juyanguai22"}));
	add_items(({ROOT "/gamelib/clone/npc/shierxianshan/baixushuyao22"}));
	add_items(({ROOT "/gamelib/clone/npc/shierxianshan/xiaopanlong22"}));
	add_items(({ROOT "/gamelib/clone/npc/shierxianshan/huoyunshou22"}));
}
