#include <globals.h>
#include <gamelib/include/gamelib.h>
inherit WAP_ROOM;
string room_race="human";
static int room_level=140;
void create(){
	name=object_name(this_object());
	name_cn="云霄洞十一层";
	desc="\n";
	exits["south"]=ROOT "/gamelib/d/shierxianjing/yunxiaodongshiceng";
	exits["north"]=ROOT "/gamelib/d/shierxianjing/yunxiaodongshierceng";
	desc+="[走出洞穴:qge74hye "+"shierxianjing/yunxiaodongyiceng"+"]\n";
	add_items(({ROOT "/gamelib/clone/npc/shierxianshan/qingyunshou17"}));
	add_items(({ROOT "/gamelib/clone/npc/shierxianshan/weilong17"}));
	add_items(({ROOT "/gamelib/clone/npc/shierxianshan/leiniao17"}));
	add_items(({ROOT "/gamelib/clone/npc/shierxianshan/linggui17"}));
}
