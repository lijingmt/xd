	add_items(({ROOT "/gamelib/clone/npc/waihai/yuansanyinluo41"}));
	add_items(({ROOT "/gamelib/clone/npc/waihai/dingcihaikui41"}));
	add_items(({ROOT "/gamelib/clone/npc/waihai/yinghudieyu41"}));
	add_items(({ROOT "/gamelib/clone/npc/waihai/zihaidaijing41"}));
	add_items(({ROOT "/gamelib/clone/npc/waihai/hongpaolongxia41"}));
	add_items(({ROOT "/gamelib/clone/npc/waihai/heibaibanyu41"}));
}
