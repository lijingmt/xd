#include <globals.h>
#include <gamelib/include/gamelib.h>
inherit WAP_ROOM;
int clear = 0;
void create(){
	name=object_name(this_object());
	set_room_type("fb");
	name_cn="ÆÇÉ¢Ö®ÑÛ";
	desc="\n";
	add_items(({ROOT "/gamelib/clone/npc/nianshou/canrennianshou"}));
	add_items(({ROOT "/gamelib/clone/npc/nianshou/liudaocanrennianshou"}));
	add_items(({ROOT "/gamelib/clone/npc/nianshou/wusejiecanrennianshou"}));
	add_items(({ROOT "/gamelib/clone/npc/nianshou/lisanjiecanrennianshou"}));
}
string query_links(){
	object player=this_player();
	string tmp="";
	tmp += "[Àë¿ª»Ã¾³:fb_leave posanzhidi_m]\n";
	return tmp;
}
int check_if_clear(){
	if(clear == 1)
		return 1;
	array(object) all_obj = all_inventory(this_object());
	foreach(all_obj,object ob){
		if(ob->is("npc")){
			clear = 0;
			break;
		}
		else
			clear = 1;
	}
	return clear;
}