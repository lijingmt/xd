	performs_attack[2]=14;
	performs_attack[3]=18;
	performs_attack[4]=22;
	performs_attack[5]=26;
	skill_type+=({"wuyao"});
	performs_level_limit[1]=25;
	performs_level_limit[2]=32;
	performs_level_limit[3]=35;
	performs_level_limit[4]=38;
	performs_level_limit[5]=41;
}
